<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="it.unipi.lsmd.spotify2.controllers.admin.genreDistributionsController" fx:id="vBox" alignment="CENTER" spacing="20">
    <HBox alignment="CENTER" style=" -fx-background-color: black;">
        <ImageView>
            <Image url="/img/logoSpotify2_1.png"/>
        </ImageView>
    </HBox>
    <Label text="Genre distributions" styleClass="title-text"/>

    <HBox alignment="CENTER">
        <Button text="Back" onAction="#handleBackButtonAction"/>
    </HBox>

    <ListView fx:id="genreDistributions" prefHeight="450" fixedCellSize="32.5"/>
</VBox>
